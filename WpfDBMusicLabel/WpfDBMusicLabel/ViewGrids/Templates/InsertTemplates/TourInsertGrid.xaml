<UserControl x:Class="WpfDBMusicLabel.ViewGrids.Templates.InsertTemplates.TourInsertGrid"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfDBMusicLabel.ViewGrids.Templates.InsertTemplates"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Name="TourInsert" 
        Margin="0,1,240,0" 
        Visibility="Collapsed">
        <Label Name="TourProjectLabel" 
            Content="Seleziona luoghi:" 
            HorizontalAlignment="Left" 
            Margin="10,10,0,0" 
            VerticalAlignment="Top" 
            FontSize="15"/>
        <ListBox Name="TourLB" 
            d:ItemsSource="{d:SampleData ItemCount=5}" 
            Margin="137,10,125,10"/>
        </Grid>
</UserControl>
